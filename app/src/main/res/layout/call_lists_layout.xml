<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/trrrrr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Trrrrrr"
            android:layout_marginTop="5dp"
            android:textColor="#000"
            android:layout_marginLeft="10dp"
            />
        <TextView
            android:id="@+id/nounit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/trrrrr"
            android:text="No Unit"
            android:layout_marginLeft="10dp"
            android:textSize="15dp"
            android:layout_marginTop="5dp"/>

        <ImageView
            android:id="@+id/profileimage"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:src="@drawable/image_bottombar_profile"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:layout_marginTop="5dp"
            />


        <TextView
            android:id="@+id/bar"
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:text="0"
            android:textSize="10sp"
            android:layout_below="@+id/profileimage"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="#ffaabb"
            />

        <TextView
            android:id="@+id/deadline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/bar"
            android:text="Deadline"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            />
        <TextView
            android:id="@+id/notset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/deadline"
            android:text="Not set"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="2dp"
            />
        <TextView
            android:id="@+id/accounts"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/bar"
            android:text="Accounts"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="5dp"
            android:layout_toRightOf="@+id/deadline"
            />
        <TextView
            android:id="@+id/accountsnmbr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/accounts"
            android:layout_toRightOf="@+id/notset"
            android:text="1"
            android:layout_marginLeft="70dp"
            android:layout_marginTop="5dp"
            android:textColor="#000"
            />
        <TextView
            android:id="@+id/contacts"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/bar"
            android:text="Contacts"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="5dp"
            android:layout_toRightOf="@+id/accounts"
            />
        <TextView
            android:id="@+id/contactsnmbr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/contacts"
            android:layout_toRightOf="@+id/accountsnmbr"
            android:text="2"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="5dp"
            />
        <ImageView

            android:layout_width="20dp"
            android:layout_height="15dp"
            android:id="@+id/callog_imageg"
            android:src="@drawable/calloggreen"
            android:layout_below="@+id/bar"
            android:layout_marginTop="5dp"
            android:layout_toRightOf="@+id/contacts"
            android:layout_marginLeft="10dp"/>
        <TextView
            android:id="@+id/calloggnmbr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/callog_imageg"
            android:layout_toRightOf="@+id/contactsnmbr"
            android:text="0"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="5dp"
            android:textColor="#000"
            />
        <ImageView

            android:layout_width="20dp"
            android:layout_height="15dp"
            android:id="@+id/callog_imager"
            android:src="@drawable/callogred"
            android:layout_below="@+id/bar"
            android:layout_marginTop="5dp"
            android:layout_toRightOf="@+id/callog_imageg"
            android:layout_marginLeft="5dp"/>
        <TextView
            android:id="@+id/callognmbrr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/callog_imager"
            android:layout_toRightOf="@+id/calloggnmbr"
            android:text="0"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="5dp"
            android:textColor="#000"
            />
        <ImageView

            android:layout_width="20dp"
            android:layout_height="15dp"
            android:id="@+id/img_calendar"
            android:src="@drawable/calendar"
            android:layout_below="@+id/bar"
            android:layout_marginTop="5dp"
            android:layout_toRightOf="@+id/callog_imager"
            android:layout_marginLeft="5dp"/>
        <TextView
            android:id="@+id/calnmbr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/img_calendar"
            android:layout_toRightOf="@+id/callognmbrr"
            android:text="0"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="5dp"
            android:textColor="#000"
            />
        <ImageView
            android:layout_width="20dp"
            android:layout_height="17dp"
            android:id="@+id/img_moneybag"
            android:src="@drawable/money"
            android:layout_below="@+id/bar"
            android:layout_marginTop="5dp"
            android:layout_toRightOf="@+id/img_calendar"
            android:layout_marginLeft="5dp"/>
        <TextView
            android:id="@+id/moneynmbr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/img_moneybag"
            android:layout_toRightOf="@+id/calnmbr"
            android:text="0"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="4dp"
            android:textColor="#000"
            />

    </RelativeLayout>
</RelativeLayout>